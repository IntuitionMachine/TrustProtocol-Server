{
  "name": "tinyapp",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "production:start": "node ./dist/app.js",
    "inspect": "babel-node app.js --inspect-brk",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:migrate"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.1.4",
    "apollo-client": "^1.9.3",
    "apollo-client-preset": "0.1.0-1",
    "apollo-server-express": "^1.1.7",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "ethereumjs-util": "^5.1.2",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-client": "^2.0.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.5.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.9.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "regenerator-runtime": "^0.11.0",
    "sequelize": "^4.17.0",
    "sequelize-cli": "^3.0.0",
    "sequelize-migration-pg-extras": "^1.0.1",
    "web3": "^1.0.0-beta.24"
  },
  "engines": {
    "node": "8.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "rimraf": "^2.6.2"
  }
}
